{
  "name": "trpc-transformer",
  "version": "1.0.3",
  "description": "Simple, quick and reliable transformer for tRPC based on superjson and devalue",
  "repository": "icflorescu/trpc-transformer",
  "bugs": {
    "url": "https://github.com/icflorescu/trpc-transformer/issues"
  },
  "type": "module",
